{
  "name": "aurelia-materialize-bridge",
  "version": "2.0.0",
  "registry": "github",
  "description": "Aurelia 2 interface to http://materializecss.com/",
  "keywords": [
    "aurelia",
    "plugin",
    "materialize"
  ],
  "homepage": "https://github.com/aurelia-ui-toolkits/aurelia-materialize-bridge",
  "bugs": {
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-materialize-bridge/issues"
  },
  "license": "MIT",
  "author": "aurelia-ui-toolkits",
  "standard-version": {
    "infile": "doc/CHANGELOG.md"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -b",
    "postbuild": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/esnext"
  },
  "main": "dist/esnext/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-materialize-bridge"
  },
  "devDependencies": {
    "@aurelia/kernel": "^0.6.0",
    "@aurelia/runtime": "^0.6.0",
    "@aurelia/runtime-html": "^0.6.0",
    "@types/materialize-css": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.1",
    "materialize-css": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "@aurelia/kernel": "^0.6.0",
    "@aurelia/runtime-html": "^0.6.0",
    "materialize-css": "^1.0.0",
    "tslib": "^1.11.0"
  }
}
